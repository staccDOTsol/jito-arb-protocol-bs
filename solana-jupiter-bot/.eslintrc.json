{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": "error",
        "react/prop-types": "off" 

    },
    "plugins": [
        "react",
        "prettier"
    ]
}
